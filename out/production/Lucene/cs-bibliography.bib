  ee = {http://dx.doi.org/10.1109/BigData.2016.7840607},
  interhash = {05f4aac66392dade3628b9a4c882df87},
  intrahash = {4c6f8f8f6efb9a71a1d33b07c3caaea2},
  isbn = {978-1-4673-9005-7},
  keywords = {NoSQL},
  pages = {214-222},
