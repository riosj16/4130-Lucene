  journal = {Science},
  keywords = {Learning Sleep},
  month = may,
  number = 6287,
  pages = {812 - 816},
  timestamp = {2019-03-25T17:09:44.000+0100},
  title = {Causal evidence for the role of {REM} sleep theta rhythm in contextual memory consolidation},
  volume = 352,
  year = 2016
}

