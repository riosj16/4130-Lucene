  intrahash = {3000ed9bbe28635b9607b7981fbda07b},
  journal = {Foundations and Trends{\textregistered} in Information Retrieval},
  keywords = {BM25 IR},
  number = 4,
  pages = {333- -- 389},
  publisher = {Now Publishers},
