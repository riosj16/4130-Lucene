  interhash = {a1ac721710dfe1a3da0fdc4d52ccef22},
  intrahash = {b4bfb2088d4a9d15fd548b922f5929a8},
  journal = {Synthesis Lectures on Information Concepts, Retrieval, and Services},
  keywords = {IR SynthesisLecture},
