  interhash = {816bec9f26a78ca24e687a355adb22ca},
  intrahash = {9de83c8af8345214c7c71e9b18a1ab3c},
  keywords = {MicroBlog NamedEntityRecognition},
  pages = {5--12},
  publisher = {ACM},
  series = {SEM '14},
  timestamp = {2019-03-25T17:14:02.000+0100},
