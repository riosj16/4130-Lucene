  interhash = {eb639c3520f6a7eb558f881085062fad},
  intrahash = {4b0469b4a1cac2a9eda8d32f3aa940ed},
  keywords = {DiagnosticAnalytics EDM LearningAnalytics VisualAnalytics},
  pages = {93--98},
  publisher = {ACM},
  series = {LAK '11},
  timestamp = {2019-03-25T17:12:29.000+0100},
