  interhash = {566639e455b996ba22f2608a4c73f1cf},
  intrahash = {ec80b849cee9ad89a5f3849176b4a58f},
  journal = {Synthesis Lectures on Human-Centered Informatics},
  keywords = {Mathematics SynthesisLecture},
  number = 1,
