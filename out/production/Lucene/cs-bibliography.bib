  keywords = {NoSQL},
  pages = {239-251},
  publisher = {ACM},
  timestamp = {2019-03-15T17:08:25.000+0100},
  title = {Have Your Data and Query It Too: From Key-Value Caching to Big Data Management.},
  url = {http://dblp.uni-trier.de/db/conf/sigmod/sigmod2016.html#BorkarMSC16},
  year = 2016
}

