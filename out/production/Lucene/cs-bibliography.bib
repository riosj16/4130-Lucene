  journal = {Computers & Education},
  keywords = {EDM LearningAnalytics},
  number = 2,
  pages = {588 - 599},
  timestamp = {2019-03-25T17:09:32.000+0100},
  title = {Mining LMS data to develop an ‚Äúearly warning system‚Äù for educators: A proof of concept},
