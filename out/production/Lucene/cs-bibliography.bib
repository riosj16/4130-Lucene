  timestamp = {2019-03-25T17:13:02.000+0100},
  title = {Discovery of complex glitch patterns: A novel approach to Quantitative Data Cleaning},
  url = {http://ieeexplore.ieee.org/xpl/articleDetails.jsp?arnumber=5767864},
