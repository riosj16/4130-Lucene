  doi = {10.1145/2629568},
  interhash = {d8bb9030ba8ccadbbaa9bc3666841f08},
  intrahash = {51c871742b69ccabc1f70415b32d637c},
  issn = {1936-1955},
  issue_date = {August 2014},
  journal = {J. Data and Information Quality},
  keywords = {DataQuality ProcessModelingLanguage},
