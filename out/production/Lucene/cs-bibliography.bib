  interhash = {85d96bac0efdf2b84d3fca101f3b1432},
  intrahash = {8cab24da6556dbfc42059b03d23b26cf},
  journal = {Synthesis Lectures on Information Concepts, Retrieval, and Services},
  keywords = {FacetedSearch IR SynthesisLecture},
  number = 1,
