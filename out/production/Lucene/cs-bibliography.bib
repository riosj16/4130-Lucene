  keywords = {NoSQL},
  pages = {295-310},
  publisher = {ACM},
  timestamp = {2019-03-15T17:08:25.000+0100},
  title = {Automatic Generation of Normalized Relational Schemas from Nested Key-Value Data.},
  url = {http://dblp.uni-trier.de/db/conf/sigmod/sigmod2016.html#DiScalaA16},
