  author = {Mirkin, Boris Grigorievitch},
  biburl = {https://www.bibsonomy.org/bibtex/21a32a1d0fb301d185a016a099f0f3d7d/vngudivada},
  interhash = {f76e2046ea257f88682f722e197e6cca},
