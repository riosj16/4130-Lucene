  intrahash = {af27933bfe668d02b25cad5ec0366859},
  keywords = {Book Learning ParExcellence},
  publisher = {Belknap Press},
  timestamp = {2019-03-25T17:10:07.000+0100},
  title = {Make it stick: the science of successful learning},
  year = 2014
}

