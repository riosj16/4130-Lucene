  intrahash = {38078854fa0f5d2a244145cf8f60bad3},
  journal = {Synthesis Lectures on Information Concepts, Retrieval, and Services},
  keywords = {IR SynthesisLecture XML},
  number = 1,
  pages = {1 -- 111},
  timestamp = {2019-03-25T17:06:11.000+0100},
  title = {XML Retrieval},
