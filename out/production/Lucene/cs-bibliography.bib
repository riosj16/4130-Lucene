  intrahash = {5826c0c386e1cd304fc9b570a2b683b2},
  issn = {1043-4046},
  journal = {Advances in Physiology Education},
  keywords = {Healthcare STEM Simulation},
  number = 3,
  pages = {210--215},
  publisher = {American Physiological Society},
