  publisher = {ACM},
  series = {UIST '15},
  timestamp = {2019-03-25T17:10:07.000+0100},
  title = {Codeopticon: Real-Time, One-To-Many Human Tutoring for Computer Programming},
  url = {http://doi.acm.org/10.1145/2807442.2807469},
  year = 2015
}

