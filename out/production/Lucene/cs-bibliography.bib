  number = 2,
  pages = {141--154},
  timestamp = {2019-03-25T17:13:22.000+0100},
  title = {Product data quality in supply chains: the case of Beiersdorf},
  url = {http://dx.doi.org/10.1007/s12525-011-0059-x},
  volume = 21,
  year = 2011
}

