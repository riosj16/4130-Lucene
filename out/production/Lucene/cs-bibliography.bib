  doi = {10.2481/dsj.14-041},
  interhash = {de687dc289646e5f1c8cb4352584dd0b},
  intrahash = {b80a3ba4fd0b01940db6300ea41e3429},
  journal = {Data Science Journal},
  keywords = {BigData DataModel NoSQL},
  number = 0,
  pages = {138 -- 157},
